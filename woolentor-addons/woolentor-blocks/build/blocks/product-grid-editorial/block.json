    "exclude_products": {
      "type": "array",
      "default": []
    },
    "exclude_out_of_stock": {
      "type": "boolean",
      "default": false
    },
    "exclude_no_image": {
      "type": "boolean",
      "default": false
    },
    "enable_filters": {
      "type": "boolean",
      "default": false
    },
    "layout": {
      "type": "string",
      "default": "grid"
    },
    "default_view_mode": {
      "type": "string",
      "default": "grid"
    },
    "columns": {
      "type": "object",
      "default": {
        "desktop": 3
